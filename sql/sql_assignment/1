select count(*) from city;


--Q1--
select * 
from city 
where 
countrycode = 'USA' 
and 
population >= 100000

--q2--
select name 
from city 
where 
countrycode = 'USA' 
and 
population >= 120000


--q3--
select * from city

--q4--
select * 
from city
where id = 1661;

--q5--
select id as city_id,
       name as city_name,
       countrycode,
       district,
       population
from city
where countrycode = 'JPN'

--q6--
select name as city_name
from city
where countrycode = 'JPN'

--Create table station and load values---

CREATE TABLE station(
   id INT NOT NULL,
  city VARCHAR(23),
  state VARCHAR(2),
  lat_n INT,
  long_n INT,
  constraint unique_id UNIQUE(id)
);

