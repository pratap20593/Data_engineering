CREATE TABLE delivery
(
  delivery_id INT,
  customer_id INT,
  order_date DATE,
  customer_pref_delivery_date DATE,
  constraint p_delivery_id PRIMARY KEY(delivery_id)
);


INSERT INTO delivery VALUES(1, 1, '2019-08-01', '2019-08-02');
