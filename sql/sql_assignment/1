create DATABASE sql_practice;

use sql_practice

drop table city

create table city (
    id int NOT NULL,
    name varchar(17) NOT NULL,
    countrycode VARCHAR(3),
    district varchar(20),
    population int,
    constraint unique_id UNIQUE(id)
);

