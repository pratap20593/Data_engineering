select * from weather;

select tmp.country_name,
        CASE 
                WHEN tmp.avt<=15 THEN 'cold'
                WHEN tmp.avt>=25 THEN 'hot'
                ELSE  'warm'
        END as weather_type
FROM
(select co.country_name,
        AVG(w.weather_state) as avt
from weather w
left join countries co
on w.country_id = co.country_id
where w.day between '2019-11-01' and '2019-11-30' 
GROUP BY country_name) tmp 
order by weather_type;

