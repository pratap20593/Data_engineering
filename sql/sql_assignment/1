--q7--

select 
    state,
    city
from station

--q8--

select distinct(city) 
from station 
where id%2 = 0

--q9--
select 
count(city) 
- 
count(distinct(city))
from station;

--10--
select city 
from 
  ((select city,
        LENGTH(city) as city_length
  from station 
  ORDER BY city_length desc,city 
  limit 1)
  UNION
  (select city,
        LENGTH(city) as city_length
  from station 
  ORDER BY city_length,city 
  limit 1)) temp_table

  --11--
  select distinct(city)
  from station
  where left(lower(city),1) in ('a','e','i','o','u');

  --12--
  select distinct(city)
  from station
  where right(lower(city),1) in ('a','e','i','o','u');

--13--
  select distinct(city)
  from station
  where left(lower(city),1) not in ('a','e','i','o','u');

  --14--
  select distinct(city)
  from station
  where right(lower(city),1) not in ('a','e','i','o','u');


--15--
  select distinct(city)
  from station
  where right(lower(city),1)  not in ('a','e','i','o','u')
        or left(lower(city),1) not in ('a','e','i','o','u');

--16--
  select distinct(city)
  from station
  where right(lower(city),1)  not in ('a','e','i','o','u')
        and left(lower(city),1) not in ('a','e','i','o','u');


--17--
create table if NOT exists product(
        product_id int,
        product_name varchar(20),
        unit_price INT,
        constraint product_id_primary PRIMARY KEY(product_id)
)

create table if not exists sales(
        seller_id int,
        product_id int,
        buyer_id int,
        sale_date DATE,
        quantity INT,
        price INT,
        constraint product_id_foreign FOREIGN KEY(product_id)
        REFERENCES product(product_id)
)

insert into product values(1,'s8',1000),
                          (2,'g4',800),
                          (3,'iphone',1400)

insert into sales values (1,1,1,'2019-01-21',2,2000),
                        (1,2,2,'2019-02-17',1,800),
                        (2,2,3,'2019-06-02',1,800),
                        (3,3,4,'2019-05-13',2,2800)

select t1.product_id,p1.product_name
from (SELECT product_id
        from sales
        where product_id not IN (SELECT product_id
        from sales
        where sale_date <= '2019-01-01' or sale_date >= '2019-03-31')) t1
INNER JOIN product p1
on t1.product_id = p1.product_id

